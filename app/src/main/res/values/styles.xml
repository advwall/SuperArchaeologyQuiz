<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#7F4B32</item>
        <item name="colorPrimaryDark">#2B1A11</item>
        <item name="colorAccent">#fff8d9</item>

    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">#2B1A11</item>
    </style>

    <style name="Base.TextAppearance.AppCompat.Title." parent="Theme.AppCompat.Light">
        <item name="android:titleTextColor">#fff8d9</item>
    </style>



    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
